{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "params": {
            "type": "object"
        }
    },
    "variables": {
        "uniqueId": "[parameters('params').uniqueId]",
        "prefix": "[parameters('params').prefix]",
        "location": "[parameters('params').location]",
        "subscription": "[parameters('params').subscription]",
        "image": "[parameters('params').image]",
        "role": "[parameters('params').role]",
        "storageUri": {
            "SAP Dev Test": {
                "ukwest": {
                    "uri": "https://azwudsapvmdiags.blob.core.windows.net/"
                }
            }
        },
        "bootDiagnostics": {
            "enabled": true,
            "storageUri": "[variables('storageUri')[variables('subscription')][variables('location')].uri]"
        }
    },
    "resources": [],
    "outputs": {
        "bootDiagnostics": {
            "value": "[variables('bootDiagnostics')]",
            "type": "object"
        }
    }
}
