{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "params": {
            "type": "object"
        }
    },
    "variables": {
        "uniqueId": "[parameters('params').uniqueId]",
        "prefix": "[parameters('params').prefix]",
        "location": "[parameters('params').location]",
        "subscription": "[parameters('params').subscription]",
        "image": "[parameters('params').image]",
        "role": "[parameters('params').role]",
        "acceleratedNetworking": {
            "Dev Test": {
                "ISU": false,
                "CRM": false,
                "PIC": false,
                "BWA": false,
                "MOB": false,
                "EWM": false,
                "JVA": false,
                "CEV": false,
                "PIN": false,
                "NGW": false
            }
        }
    },
    "resources": [],
    "outputs": {
        "enableAcceleratedNetworking": {
            "value": "[variables('acceleratedNetworking')[variables('subscription')][variables('role')]]",
            "type": "bool"
        }
    }
}
