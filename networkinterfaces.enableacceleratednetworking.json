{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "subscription": {
            "type": "string",
            "allowedValues": [
                "Dev Test"
            ]
        },
        "role": {
            "type": "string",
            "allowedValues": [
                "ISU",
                "CRM",
                "PIC",
                "BWA",
                "MOB",
                "EWM",
                "JVA",
                "CEV",
                "PIN",
                "NGW"
            ]
        }
    },
    "variables": {
        "acceleratedNetworking": {
            "Dev Test": {
                "ISU": false,
                "CRM": false,
                "PIC": false,
                "BWA": false,
                "MOB": false,
                "EWM": false,
                "JVA": false,
                "CEV": false,
                "PIN": false,
                "NGW": false
            }
        }
    },
    "resources": [],
    "outputs": {
        "enableAcceleratedNetworking": {
            "value": "[variables('acceleratedNetworking')[parameters('subscription')][parameters('role')]]",
            "type": "bool"
        }
    }
}
